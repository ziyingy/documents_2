<!-- PJG 007 0104 intable -->
Diethyl phthalate 
<!-- PJG 007 0104 intable -->
Remove Previously Recommended Chemical 
<!-- PJG 007 0104 intable -->
5/94 
<!-- PJG 0012 frnewline -->
90&hyph;30&hyph;2 
<!-- PJG 007 0104 intable -->

<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=3 -->
N-
<!-- PJG /ITAG -->

<!-- PJG ITAG l=01 g=1 f=1 -->
