<!-- PJG ITAG l=10 g=1 f=1 -->
 Notice of Approved Tribal-State Compact.
<!-- PJG /ITAG -->

<!-- PJG ITAG l=59 g=1 f=1 -->
 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->
