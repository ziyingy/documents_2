<!-- PJG ITAG l=55 g=1 f=1 -->
Antarctic Marine Living Marine Resources Convention Act of 1984 
<!-- PJG 0012 frnewline -->

<!-- PJG /ITAG -->

<AGENCY>
<!-- PJG ITAG l=10 g=1 f=2 -->
