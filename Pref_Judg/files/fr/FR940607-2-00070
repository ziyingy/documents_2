<!-- PJG ITAG l=10 g=1 f=2 -->
DATES: 
<!-- PJG /ITAG -->

<!-- PJG ITAG l=10 g=1 f=1 -->
June 16, 1994, 9 a.m. to 5 p.m.; and June 17, 1994, 9 a.m. to 12:30 p.m.
<!-- PJG /ITAG -->

<!-- PJG QTAG 02 -->
<!-- PJG /QTAG -->

<!-- PJG 0012 frnewline -->

